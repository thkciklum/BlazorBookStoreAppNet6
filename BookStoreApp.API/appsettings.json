{
  "Serilog": {
    "NibimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Informatoin"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "Path": "./Logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "seq",
        "Args": { "ServerUrl": "http://localhost:5341/" }
      }
    ]
  },
  "AllowedHosts": "*"
}
